src_alu_a, src_alub : ALUSrcD
reg_wr_en : RegWriteD
mem_wr_en :  MemWriteD
mem_byte_sel : 
alu_ctrl : ALUControlD



RD1 : rs1_data
RD2 : rs2_data
PCD : PCD
Rd 	: new flipflop
ImmExtD : imm
PCPlus4D : PCPlus4D


//em_register

reg_wr_en	: RegWrite
src_rd		: ResultSrc
